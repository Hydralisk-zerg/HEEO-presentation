{
  "name": "heeo-presentation-react",
  "version": "1.0.0",
  "description": "HEEO 2025 презентация на React с Ant Design",
  "homepage": "https://hydralisk-zerg.github.io/HEEO-presentation",
  "scripts": {
    "start": "webpack serve --mode development",
    "dev": "webpack serve --mode development",
    "build:local": "webpack --mode production",
    "build": "cross-env GITHUB_PAGES=true webpack --mode production",
    "predeploy": "npm run build",
    "deploy": "npm run build && cd dist && git init && git add . && git commit -m \"Deploy to GitHub Pages\" && git branch -M gh-pages && git remote add origin https://github.com/Hydralisk-zerg/HEEO-presentation.git && git push -f origin gh-pages && cd .. && Remove-Item -Recurse -Force \"dist\\.git\"",
    "serve": "npx serve -s dist -l 3000"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "antd": "^5.12.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "gh-pages": "^6.1.0",
    "html-webpack-plugin": "^5.6.3",
    "serve": "^14.2.4",
    "style-loader": "^4.0.0",
    "vite": "^5.0.8",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
